                                  <p className="text-gray-600 text-sm">
                                    {partner.company}
                                  </p>
                                </div>
                              </div>

                              <div className="space-y-3 mb-4">
                                <div className="flex items-center text-gray-600">
                                  <MapPin className="w-4 h-4 mr-2 text-gray-400" />
                                  <span className="text-sm">
                                    {partner.city}, {partner.state}
